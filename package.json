{
  "name": "StocksBar",
  "version": "1.1.2",
  "description": "A minimal Electron application to watch stocks on bar",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --mac --win"
  },
  "repository": {
    "type": "git",
    "url": "null"
  },
  "keywords": [
    "Stocks",
    "Bar"
  ],
  "author": "emtry",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^6.0.0",
    "electron-builder": "^21.2.0"
  },
  "dependencies": {
    "electron-store": "^4.0.0",
    "iconv-lite": "^0.5.0",
    "request": "^2.88.0",
    "storejs": "^1.0.25",
    "wait-until": "0.0.2"
  },
  "build": {
    "mac": {
      "icon": "images/StocksBar.png",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "icon": "images/icon512.ico",
      "target": [
        "zip"
      ]
    }
  }
}
